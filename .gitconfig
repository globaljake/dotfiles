# This is Git's per-user configuration file.
[alias]
    oops = reset --soft HEAD~1
    branch-delete-all = branch | grep -v "*" | xargs git branch -D
    branch-sorted = branch --sort=-committerdate
[core]
    editor = nvim
[pull]
    rebase = true
[push]
    default = current
